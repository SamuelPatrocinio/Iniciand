       System.out.println("Digite um numero");
       N1=Recebe.nextInt();
       if(N1%2==00){
           System.out.println("O numero digitado é par "+N1); 
       }else{
       System.out.println("O numero digitado é ímpar "+N1);
       }
4)
   
        float N1,N2,N3,soma,Media;
        
        System.out.print("Digite a primeira nota do aluno: ");
        N1=Recebe.nextInt();
        System.out.print("Digite a segunda nota do aluno: ");
        N2=Recebe.nextInt(); 
        System.out.print("Digite a treceira nota do aluno:");
        N3=Recebe.nextInt();
        soma=N1+N2+N3;
        Media= soma/3;
        if(Media>=7){
            System.out.println("Aluno aprovado ");
            System.out.println("A media do aluno é "+Media);
        }else{
            System.out.println("Aluno reprovado ");
            System.out.println("A media do aluno é "+Media);
        
        }
        

6)

        float N1,N2,N3,soma,Media;
        int Con=1,R;
        System.out.println("Digite o numero de ");
        R=Recebe.nextInt();
        while(R>=Con){
        System.out.println("Digite a primeira nota do aluno: ");
         N1=Recebe.nextInt();
        System.out.print("Digite a segunda nota do aluno: ");
        N2=Recebe.nextInt(); 
        System.out.print("Digite a treceira nota do aluno:");
        N3=Recebe.nextInt();
        soma=N1+N2+N3;
        Media= soma/3;
        if(Media>=7){
            System.out.println("Aluno aprovado ");
            System.out.println("A media do aluno é "+Media);
        }else{
            System.out.println("Aluno reprovado ");
            System.out.println("A media do aluno é "+Media);
        }
       
        Con++;
        }
